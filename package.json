{
  "name": "openapi-tree-shaker",
  "private": false,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "tree-shake": "tsx src/cli.ts",
    "build:cli": "tsc --project tsconfig.node.json",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepare": "npm run build:cli",
    "prepublishOnly-disabled": "npm run test && npm run lint"
  },
  "bin": {
    "openapi-tree-shake": "./dist/cli.js"
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "dependencies": {
    "js-yaml": "^4.1.0",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.11.30",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^1.4.0",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.7.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "vitest": "^1.4.0"
  },
  "description": "A tool to tree-shake OpenAPI specifications by removing unused paths and components",
  "keywords": [
    "openapi",
    "swagger",
    "tree-shake",
    "optimization"
  ],
  "author": "StackBlitz",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/stackblitz/openapi-tree-shaker"
  },
  "bugs": {
    "url": "https://github.com/stackblitz/openapi-tree-shaker/issues"
  },
  "homepage": "https://github.com/stackblitz/openapi-tree-shaker#readme",
  "engines": {
    "node": ">=18"
  }
}